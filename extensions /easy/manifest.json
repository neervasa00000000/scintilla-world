{
  "manifest_version": 3,
  "name": "ExtractFlow // Soft UI",
  "version": "1.0.1",
  "description": "Extract emails, phones and names from pasted text, then export as CSV.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "ExtractFlow"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
